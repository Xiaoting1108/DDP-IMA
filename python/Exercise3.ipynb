   "source": [
    "string = input()\n",
    "count1 = 0\n",
    "count2 = 0\n",
    "for i in string:\n",
    "      if(i.islower()):\n",
    "            count1=count1+1\n",
    "      elif(i.isupper()):\n",
    "            count2=count2+1\n",
    "\n",
    "print('Uppercase:', count2, ',', 'Lowercase:', count1)\n"
